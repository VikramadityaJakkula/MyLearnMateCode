                    <TextBlock x:Name="PopUpText" FontSize="14"/>
                    <Button x:Name="PopUpButton" Height="30" Width="80" Content="Close" Click="PopUpButton_Click" />
                </StackPanel>
            </Border>
        </Popup>
        
        <InkPresenter x:Name="MyIP" Height="490" Width="1030" 
              Margin="19,130,0,0"
              MouseLeftButtonDown="MyIP_MouseLeftButtonDown" 
              LostMouseCapture="MyIP_LostMouseCapture" 
              MouseMove="MyIP_MouseMove" 
              Background="Transparent" Opacity="1" Canvas.Top="33"  Canvas.Left="12"
                       >



            <InkPresenter.Strokes>
                <StrokeCollection>
                    <Stroke>
                        <Stroke.DrawingAttributes>
                            <DrawingAttributes Color="#FFFFEB00" OutlineColor="Yellow" Width="10" Height="5"/>
