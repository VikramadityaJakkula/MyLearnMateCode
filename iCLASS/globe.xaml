            <Button.Template>
                <ControlTemplate TargetType="Button">
                    <Border Background="black" CornerRadius="12" Name="Border">
                        <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />
                    </Border>
                </ControlTemplate>
            </Button.Template>
        </Button>

        <dataInput:Label Height="32" HorizontalAlignment="Left" Margin="0,278,0,0" Name="label21" VerticalAlignment="Top" Width="530" Content="Copyright © 2010 DataMiningTools Inc. All rights reserved." Canvas.Top="382" Canvas.Left="21" Foreground="White" RenderTransformOrigin="0,0" FontWeight="Bold" />





        <Image Height="165" HorizontalAlignment="Left"  Name="image8" Stretch="Fill" VerticalAlignment="Top" Width="0" Source="Logo.jpg
               " />
        <Grid x:Name="LayoutRoot" Background="Black" Height="500" Width="750" Canvas.Left="150" Canvas.Top="150" OpacityMask="Orange">


            <Grid x:Name="Cover">
                <TextBlock Text="Spin" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White" FontFamily="Verdana" FontSize="48" FontWeight="Bold"/>
                <dataInput:Label Content="North " FontSize="30" FontWeight="Bold" Foreground="#FF27DB27" Name="label4" RenderTransformOrigin="0,0" Margin="322,0,310,461" />
                <dataInput:Label Content="South " FontSize="30" FontWeight="Bold" Foreground="#FF27DB27" Name="label2" RenderTransformOrigin="0,0" Margin="340,462,300,0" />
            </Grid>
        </Grid>
        <dataInput:Label Canvas.Left="908" Canvas.Top="360" Content="East" FontSize="30" FontWeight="Bold" Foreground="#FF27DB27" Height="47" Name="label3" RenderTransformOrigin="0,0" Width="99" />
        <dataInput:Label Canvas.Left="45" Canvas.Top="360" Content="West" FontSize="30" FontWeight="Bold" Foreground="#FF27DB27" Height="47" Name="label1" RenderTransformOrigin="0,0" Width="99" />
